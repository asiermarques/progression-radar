{
  "name": "progression-radar",
  "version": "0.1.14",
  "private": false,
  "main": "index.js",
  "files": [
    "/lib"
  ],
  "dependencies": {
    "@friends-of-js/yaml-loader": "^1.0.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
