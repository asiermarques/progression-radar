      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.5",
    "@babel/cli": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "prettier": "1.19.1"
  }
